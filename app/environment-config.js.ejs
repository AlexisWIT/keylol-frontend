(function () {
    "use strict";

    keylolApp.config([
        "apiEndpointProvider", "httpBatchConfigProvider",
        function(apiEndpointProvider, httpBatchConfigProvider){
            apiEndpointProvider.setEndPoint("<%= apiEndpoint %>");

            httpBatchConfigProvider.setAllowedBatchEndpoint("<%= apiEndpoint %>", "<%= apiEndpoint %>batch", {
                batchRequestCollectionDelay: 10
            });
        }
    ]);
})();